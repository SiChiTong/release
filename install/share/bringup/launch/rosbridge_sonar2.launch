<?xml version="1.0"?>
<launch>
    <node name="rosbridge_sonar" pkg="rosbridge_driver" type="rosbridge_driver" output="screen">
        <rosparam>
            port_chain:
              - name: sonar
                type: serial
                serial_port: /dev/usb_right
                serial_baud: 9600
                serial_stop: 1
            topic_chain:
              - name: waypoint_user_pub
                type: subscribe
              - name: sonar_test
                type: event
                action_chain:
                  - name: sonar
                    type: write
                    byte: [0x55]
                  - name: sonar
                    type: read
                    byte: [sonar_data, big]
                    time_out: 0.2
                echo: sonar_data
              - name: diagnostics
                type: publish
        </rosparam>
    </node>
</launch>
