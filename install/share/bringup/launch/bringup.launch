<?xml version="1.0"?>
<launch>
    <arg name="robot_env" default="$(env ROS_FILENAME)"/>
    <arg name="robot_pkg" default="$(find bringup)"/>
    <arg name="is_config" default="true"/>

    <include file="$(arg robot_pkg)/config/$(arg robot_env).launch">
        <arg name="robot_env" value="$(arg robot_env)"/>
        <arg name="robot_pkg" value="$(arg robot_pkg)"/>
        <arg name="is_config" value="false"/>
    </include>
    
    <rosparam file="$(arg robot_pkg)/config/$(arg robot_env).yaml" if="$(arg is_config)"/>
</launch>
