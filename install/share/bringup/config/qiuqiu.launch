<?xml version="1.0"?>
<launch>
    <node name="aqmd_driver" pkg="aqmd_driver" type="aqmd_driver" output="screen">
        <rosparam>
            is_pub_tf: false
            wheel_separation: 0.234
            tty_name: /dev/ttyUSB0
        </rosparam>
    </node>

    <node name="teleop_keyboard" pkg="teleop_keyboard" type="teleop_keyboard" output="screen"/>
</launch>